{
    "SystemId": "MDCalc 44",
    "Name": "Fibrotic NASH Index (FNI)",
    "description": "Screens for fibrotic NASH in individuals at high risk for NAFLD.",
    "inputs": [
        {
            "name": "AST",
            "unit": "U/L",
            "value": "30",
            "type": "float",
            "required": true
        },
        {
            "name": "HbA1c",
            "unit": "% gly Hgb",
            "value": "5",
            "type": "float",
            "required": true
        },
        {
            "name": "HDL",
            "unit": "mg/dL",
            "value": "80",
            "type": "float",
            "required": true
        }
    ],
    "steps": [
        {
            "name": "calculate_x",
            "type": "calculation",
            "equation": "x = -10.33 + 2.54 * ln(AST) + 3.86 * ln(HbA1c) - 1.66 * ln(HDL)"
        },
        {
            "name": "calculate_FNI",
            "type": "calculation",
            "equation": "FNI = math.exp(x) / (1 + math.exp(x))"
        }
    ],
    "transitions": [],
    "output": [
        {
            "name" : "predicted_probability",
            "unit" : "%",
            "description" : "Predicted probability of fibrotic NASH."
        },
        {
            "name": "FNI_message",
            "description": "Your predicted probability of fibrotic NASH is X%. Fibrotic NASH can be ruled out if the FNI value is â‰¤ 0.10."
        }
    ]
}
